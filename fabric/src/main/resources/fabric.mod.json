{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "sources": "${sources_url}",
    "issues": "${issues_url}"
  },
  "license": "${license}",
  "icon": "${mod_id}_icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "com.illusivesoulworks.customfov.CustomFovFabricMod"
    ]
  },
  "accessWidener" : "${mod_id}.accesswidener",
  "mixins": [
    "${mod_id}.mixins.json",
    "${mod_id}.fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14",
    "fabric-api": "*",
    "minecraft": "${minecraft_version_range_alt}",
    "java": ">=17"
  }
}
  